{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://aurora-rise-production.up.railway.app/api/:path*"
    }
  ],
  "routes": [
    {
      "src": "/downloads/(.*)",
      "dest": "/downloads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

